{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "eetezadi/click2share",
	"icon": "testimonial",
	"version": "1.1.5",
	"title": "Click 2 Share",
	"category": "widgets",
	"description": "Gutenberg Block Plugin to display a shareable post on Social Media like Meta Threads.",
	"keywords": [ "social share", "Tweet", "click2share", "social media", "threads" ],
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"post": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": ""
		},
		"pageLink": {
			"type": "string"
		},
		"userName": {
			"type": "string"
		},
		"shareString": {
			"type": "string"
		},
		"linkLabel": {
			"type": "string"
		},
		"theme": {
			"type": "string"
		},
		"backgroundColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		}
	},
	"styles": [
		{
			"name": "light",
			"label": "Light",
			"isDefault": true
		},
		{
			"name": "dark",
			"label": "Dark"
		}
	],
	"textdomain": "click-2-share",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
